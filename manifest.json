{
    "manifest_version": 2,
    
    "name": "SpotifyOpener",
    "description": "Open Spotigy Application insted of the web player"
    ,

    "version": "0.1",

    "permissions": [
    "https://play.spotify.com/",
    "http://play.spotify.com/",
    "activeTab"
    ],
    "browser_action": { 
    "default_icon": "icon.png",
    "default_popup": "popup.html"
    }
    ,
    "background": {
    "page": "background.html"
    }

}

